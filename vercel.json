{
  "version": 2,
  "buildCommand": "pnpm build",
  "outputDirectory": "_site",
  "rewrites": [
    {
      "source": "/docs/assets/:path*",
      "destination": "https://chassis-assets.vercel.app/:path*"
    },
    {
      "source": "/docs/css/:path*",
      "destination": "https://chassis-css.vercel.app/:path*"
    },
    {
      "source": "/docs/tokens/:path*",
      "destination": "https://chassis-tokens.vercel.app/:path*"
    },
    {
      "source": "/docs/figma/:path*",
      "destination": "https://chassis-figma.vercel.app/:path*"
    },
    {
      "source": "/docs/icons/:path*",
      "destination": "https://chassis-icons.vercel.app/docs/icons/:path*"
    },
    {
      "source": "/assets/cx-icons/:path*",
      "destination": "https://chassis-icons.vercel.app/assets/cx-icons/:path*"
    },
    {
      "source": "/docs/assets/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-assets-staging.vercel.app/:path*"
    },
    {
      "source": "/docs/css/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-css-staging.vercel.app/:path*"
    },
    {
      "source": "/docs/tokens/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-tokens-staging.vercel.app/:path*"
    },
    {
      "source": "/docs/figma/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-figma-staging.vercel.app/:path*"
    },
    {
      "source": "/docs/icons/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-icons-staging.vercel.app/docs/icons/:path*"
    },
    {
      "source": "/assets/cx-icons/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "staging.chassis-ui.com"
        }
      ],
      "destination": "https://chassis-icons-staging.vercel.app/assets/cx-icons/:path*"
    }
  ]
}
